{
  "expo": {
    "name": "Yatou",
    "slug": "yatou-delivery",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "yatou",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logos/Logo-yatou-01.png",
      "resizeMode": "contain",
      "backgroundColor": "#DC2626"
    },
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#DC2626",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "YATOU - Nouvelle commande"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyBOwNDFwx9EerTB29GCdwyCyaaQIDgs9UI"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Cette application utilise votre position pour vous localiser sur la carte et calculer les itinéraires de livraison."
      },
      "entitlements": {
        "aps-environment": "production"
      },
      "bundleIdentifier": "com.yohan.yatou"
    },
    "android": {
      "package": "com.yohan.yatou",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBOwNDFwx9EerTB29GCdwyCyaaQIDgs9UI"
        }
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE"
      ],
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/logos/Logo-yatou-01.png",
      "config": {
        "googleMapsApiKey": "AIzaSyBOwNDFwx9EerTB29GCdwyCyaaQIDgs9UI"
      }
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-dev-client",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Cette application utilise votre position pour vous localiser sur la carte et calculer les itinéraires de livraison."
        }
      ],
      [
        "onesignal-expo-plugin",
        {
          "mode": "development",
          "appId": "${ONE_SIGNAL_APP_ID}",
          "androidAppId": "${ONE_SIGNAL_APP_ID}",
          "iOSAppId": "${ONE_SIGNAL_APP_ID}"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c154c066-2561-48be-bd88-bb131406475f"
      },
      "allowEmulatorNotifications": true,
      "oneSignalAppId": "${ONE_SIGNAL_APP_ID}",
      "oneSignalRestApiKey": "${ONE_SIGNAL_REST_API_KEY}"
    },
    "owner": "yohan0707"
  }
}
