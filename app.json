{
  "expo": {
    "name": "Yatou",
    "slug": "yatou-delivery",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logos/Logo-yatou-01.png",
    "scheme": "yatou",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logos/Logo-yatou-01.png",
      "resizeMode": "contain",
      "backgroundColor": "#DC2626"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyBOwNDFwx9EerTB29GCdwyCyaaQIDgs9UI"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Cette application utilise votre position pour vous localiser sur la carte et calculer les itinéraires de livraison."
      }
    },
    "android": {
      "package": "com.yohan.yatou",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBOwNDFwx9EerTB29GCdwyCyaaQIDgs9UI"
        }
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/logos/Logo-yatou-01.png",
      "config": {
        "googleMapsApiKey": "AIzaSyBOwNDFwx9EerTB29GCdwyCyaaQIDgs9UI"
      }
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-dev-client",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Cette application utilise votre position pour vous localiser sur la carte et calculer les itinéraires de livraison."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/logos/Logo-yatou-01.png",
          "color": "#DC2626",
          "defaultChannel": "default"
        }
      ],
      "@react-native-firebase/app"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c154c066-2561-48be-bd88-bb131406475f"
      }
    },
    "owner": "yohan0707"
  }
}
